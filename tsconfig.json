{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": ["es2015", "es2016", "esnext", "DOM"],
    "jsx": "react-native",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
